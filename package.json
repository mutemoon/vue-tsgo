	"version": "0.0.1",
	"publisher": "local",
	"private": true,
	"engines": {
		"vscode": "^1.88.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:vue"
	],
	"main": "dist/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "vue",
				"extensions": [
					".vue"
				],
				"configuration": "./language/vue-language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "vue",
				"scopeName": "source.vue",
				"path": "./syntaxes/vue.tmLanguage.json"
			},
			{
				"scopeName": "markdown.vue.codeblock",
				"path": "./syntaxes/markdown-vue.json",
				"injectTo": [
					"text.html.markdown"
				],
				"embeddedLanguages": {
					"source.vue": "vue"
				}
			},
			{
				"scopeName": "mdx.vue.codeblock",
				"path": "./syntaxes/mdx-vue.json",
				"injectTo": [
					"source.mdx"
				],
				"embeddedLanguages": {
					"source.vue": "vue"
				}
			},
			{
				"scopeName": "vue.directives",
				"path": "./syntaxes/vue-directives.json",
				"injectTo": [
					"source.vue",
					"text.html.markdown",
					"text.html.derivative",
					"text.pug"
				]
			},
			{
				"scopeName": "vue.interpolations",
				"path": "./syntaxes/vue-interpolations.json",
				"injectTo": [
					"source.vue",
					"text.html.markdown",
					"text.html.derivative",
					"text.pug"
				]
			},
			{
				"scopeName": "vue.sfc.style.variable.injection",
				"path": "./syntaxes/vue-sfc-style-variable-injection.json",
				"injectTo": [
					"source.vue"
				]
			}
		],
		"commands": [
			{
				"command": "vueTsgo.restartServer",
				"title": "Vue TSGo: é‡å¯ tsgo æœåŠ¡"
